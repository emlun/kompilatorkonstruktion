object Main { def main(): Unit = { if(new RealMain().realMain()){} } }

class RealMain {
  def realMain(): Bool = {
    println(returnMe<Int>(10));
    println(returnMe<String>("hej"));
    return false;
  }

  def returnMe<T>(me: T): T = {
    return me;
  }
}

