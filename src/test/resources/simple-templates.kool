object Main { def main(): Unit = { if(new RealMain().realMain()){} } }

class RealMain {
  def realMain(): Bool = {
    var mint: Maybe<Int>;
    mint = new Maybe<Int>();
    printMint(mint);
    mint.set(5);
    printMint(mint);
    mint.unset();
    printMint(mint);
    mint.set(7);
    printMint(mint);
    return false;
  }

  def printMint(mint: Maybe<Int>): Bool = {
    if(mint.isDefined()) {
      println("Value: " + mint.get());
    } else {
      println("Undefined");
    }
    return true;
  }
}

class Maybe<T> {
  var is: Bool; var value: T;
  def set(newValue: T): Maybe<T> = {
    value = newValue;
    is = true;
    return this;
  }
  def unset(): Maybe<T> = {
    is = false;
    return this;
  }
  def isDefined(): Bool = { return is; }
  def get(): T = { return value; }
}

class Pair<A, B> {
  var a: A;
  var b: B;
}
